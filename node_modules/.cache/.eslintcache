all.min.css';\nimport './App.css';\nimport { useState, useEffect } from 'react';\nimport Alert from './components/Alert';\nimport Skills from './components/Skills';\nimport Profile from './components/Profile';\nimport Practice1Tab from './components/Practice1Tab';\n\nimport {\n  BrowserRouter as Router,\n  Switch,\n  Route,\n  Link\n} from \"react-router-dom\";\n\n\nfunction App() {\n  const [mode, setMode] = useState('light');\n  const [showAlert, setShowAlert] = useState(true);\n\n  const [alert, setAlert] = useState({\n    msg: 'Grab The Amazing Offers.',\n    class: 'primary',\n    type: 'Success'\n  })\n\n  const handleCloseAlert = () => {\n    setShowAlert(false);\n  }\n\n  const toggleMode = () => {\n    setShowAlert(true);\n\n    if (mode == 'dark') {\n      setMode('light');\n      document.body.classList.remove('dark-mode');\n      setAlert({\n        msg: 'Light Mode On.',\n        class: 'primary',\n        type: 'Success'\n      })\n    } else {\n      setMode('dark');\n      document.body.classList.add('dark-mode');\n      setAlert({\n        msg: 'Dark Mode On.',\n        class: 'primary',\n        type: 'Success'\n      })\n    }\n  }\n\n  function Users() {\n    return <h2>Users</h2>;\n  }\n\n  const user = {\n    name: \"Test\",\n    lastName: \"Verma\"\n  }\n  // how to use createElement \n  // const ele = React.createElement(element.type, element.props, onclick: function() {})\n  // return ele;\n\n  // JSX code\n  return (\n    <>\n      <Router>\n        {/* passing data to child component and toggleMode is a eventlistener function here */}\n        <Header uData={user} person={{ name: 'Lin Lanying added', imageId: '1bX5QH6' }} mode={mode} toggleMode={toggleMode} />\n        <Alert showAlert={showAlert} onClose={handleCloseAlert} msg={alert.msg} class={alert.class} type={alert.type} />\n\n        <nav>\n          <ul>\n            <li>\n              <Link to=\"/skills\">Skills </Link>\n            </li>\n          </ul>\n        </nav>\n        <Practice1Tab></Practice1Tab>\n\n        {/* use client side routing */}\n        <Switch>\n          <Route exact path=\"/skills\" component={<Skills/>} />\n          <Route exact path=\"/profile\">\n            <Profile />\n          </Route>\n          <Route exact path=\"/about\">\n            <About />\n          </Route>\n          <Route exact path=\"/text-utils\">\n            <TextForm heading={'Test'} setAlert={setAlert} />\n          </Route>\n        </Switch>\n      </Router>\n    </>\n  );\n}\n\nexport default App;\n\n// class example extends React Component {\n//   render() {\n//     return html;\n//   }\n// }\n\n// ReactDOM.render(<example />, document.getElementById('root'));\n",[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/reportWebVitals.js",[],[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/Profile.js",[],[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/About.js",[],[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/Skills.js",[],[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/Banner.js",[],[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/TextForm.js",[],[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/Header.js",["64"],[],"/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/Alert.js",["65"],[],"import React, { useState } from 'react'\n\nexport default function Alert(props) {\n\n  return (\n    props.showAlert && <div>\n      <div className={`alert alert-${props.class} alert-dismissible fade show`} role=\"alert\">\n        <strong>{props.type}</strong>: {props.msg}\n        <button type=\"button\" className=\"btn-close\" onClick={props.onClose} aria-label=\"Close\"></button>\n      </div>\n    </div>\n  )\n}\n","/Users/vipinjoshi/Documents/GitHub/React-Practice-Project/src/components/Practice1Tab.js",[],[],{"ruleId":"66","severity":1,"messa